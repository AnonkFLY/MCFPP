{
  "name": "mcfpp",
  "version": "0.0.1",
  "description": "A language framework on Minecraft Java Edition datapack mcfunction improvement",
  "keywords": [
    "cli",
    "mcfunction",
    "Minecraft",
    "datapack"
  ],
  "author": "MinecraftFunctionPlusPlus",
  "license": "GPL 3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/XiLaiTL/MCFPP.git"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./build/cjs/index.cjs",
        "types": "./build/cjs/index.d.cts"
      },
      "import": {
        "default": "./build/esm/index.mjs",
        "types": "./build/esm/index.d.ts"
      }
    }
  },
  "main": "build/cjs/index.cjs",
  "types": "build/cjs/index.d.cts",
  "module": "build/esm/index.mjs",
  "type": "module",
  "files": [
    "build"
  ],
  "scripts": {
    "antlr": "antlr4ts -o ./src/antlr -visitor ./antlr/MCFPP.g4",
    "convert-to-esm": " ts-node-esm ./src/lib/JSExtUtils.ts ./src/antlr",
    "generator-antlr": "antlr4ts -o ./src/antlr -visitor ./antlr/MCFPP.g4 && ts-node-esm ./src/lib/JSExtUtils.ts ./src/antlr",
    "clean": "cd ./script && clean.sh",
    "rename-mjs": "cd ./script && rename-mjs.sh",
    "rename-cjs": "ts-node-esm ./src/lib/ToCjsUtils.ts ./build/cjs",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && npm run rename-mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && npm run rename-cjs",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch",
    "start": "node ./build/esm/index.mjs",
    "quick-start": "ts-node-esm --experimental-specifier-resolution=node ./src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/nconf": "^0.10.3",
    "antlr4ts": "0.5.0-alpha.4",
    "commander": "^10.0.0",
    "i18next": "^22.4.9",
    "i18next-fs-backend": "^2.1.1",
    "inquirer": "^9.1.4",
    "nconf": "^0.12.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitest/ui": "^0.27.2",
    "antlr4ts-cli": "0.5.0-alpha.4",
    "ts-node": "^10.9.1",
    "vitest": "^0.27.2"
  }
}
